{
  "version": 2,
  "name": "metadone-italia",
  "builds": [
    {
      "src": "src/index.js",
      "use": "@now/node-server"
    }
  ],
  "routes": [
    {
      "src": "/.*",
      "dest": "src/index.js"
    }
  ],
  "alias": ["metadone-italia"],
  "env": {
    "NODE_ENV": "production",
    "PORT": "1337",
    "CORS_ORIGIN": "https://metadone-italia.vercel.app"
  }
}
